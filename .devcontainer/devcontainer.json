{
  "name": "Vector Codespace",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "extensions": [
    "lucperkins.vrl-vscode",
    "bungcip.better-toml",
    "redhat-developer.vscode-yaml",
    "ZainChen.vscode-json"
  ],
  "forwardPorts": [8686],
  "workspaceMount": "source=${localWorkspaceFolder},target=/etc/vector,type=bind,consistency=delegated",
  "workspaceFolder": "/etc/vector",
}